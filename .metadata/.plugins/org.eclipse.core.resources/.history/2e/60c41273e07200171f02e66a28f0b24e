/** All Bullfrogs are Frogs but not all Frogs are Bullfrogs
 * 
 */
package frog;

/**
 * @author Jerilyn Mensah
 *
 */
public class Bullfrog extends Animal {
	
	public Bullfrog()
	{
		//super
		
		System.out.println("A bullfrog is born");	
	}
	
	
	@Override
	public String eat() {
		System.out.println("A bullfrog eats files");
	}
	
	@Override
	public String sleep() {
		return "A bullfrog sleeps...";
	}
	
	public String jump() {
		return "A bullfrog can jump in the air" ;
	}
	
	public String croak() {
		return "A bullfrog croaks";
	}
	

}
