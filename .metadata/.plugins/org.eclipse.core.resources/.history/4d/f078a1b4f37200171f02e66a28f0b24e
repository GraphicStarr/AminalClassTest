
public class AnimalApp {

	public static void main(String[] args) {

		Animal a = new Animal();
		print(a.eat());
		print(a.sleep());

		Bullfrog b = new Bullfrog();
		print(b.eat());
		print(b.sleep());
		print(b.croak());
		print(b.jump());
		

		Mouse m = new Mouse();
		print(m.eat());
		print(m.sleep());
		print(m.squeak());
		print(m.bite());

	}
	private static void print(String s){
		System.out.println(s);
	}
}
