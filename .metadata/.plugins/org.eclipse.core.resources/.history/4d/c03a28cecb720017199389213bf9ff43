package Fizz;

public class fizzBuzz {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Declare variables
		
		String Fizz;
		String Buzz;
		String FizzBuzz;

// Lets make a FizzBuzz loop


for (int i=1; i <=100; i++) {

	if( i % 3 == 0){                            // Multiples of 3 will have output of "Fizz"
	
		System.out.println("\nFizz");
		
	}
	else if(i % 5 == 0){                 // Multiples of 5 will have output of "Buzz"
			
			System.out.println("\nBuzz");
			
	}
	else if( i % 5 == 0 && i % 3 == 0){           //Multiples of 5 and 3 will have output of "FizzBuzz"
			
			System.out.println("\nFizzBuzz");
			
	}
	else{
			
			System.out.print(i);
			}
			
			}
		}

}
	
	


	


